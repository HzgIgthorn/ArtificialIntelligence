package de.tobi.breadthfirstsearch;

public class BFSApp {

	public static void main(String[] args) {
		Vertex v1 = new Vertex(1);
		Vertex v2 = new Vertex(2);
		Vertex v3 = new Vertex(3);
		Vertex v4 = new Vertex(4);
		Vertex v5 = new Vertex(5);
		
		v1.addNeigh(v2);
		v1.addNeigh(v4);
		v4.addNeigh(v5);
		v2.addNeigh(v3);
		
		BFS bfs = new BFS();
		bfs.bfs(v3);

	}

}
